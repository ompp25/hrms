{
	"_version": "1.5.0",
	"sap.app": {
		"id": "com.create.empEmp_creation",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/HRMS_System",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "com.create.empEmp_creation.view.Overview",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.f": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.create.empEmp_creation.i18n.i18n"
				}
			},
			"modelProdMain": {
				"dataSource": "mainService"
			
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.create.empEmp_creation.view",
				"transition": "show",
				"clearTarget": "false"
			},
			"routes": [{
				"pattern": "",
				"name": "Overview",
				"viewName": "Overview",
				"targetControl": "app",
				"targetAggregation": "pages"
			}, {
				"pattern": "second/{id}",
				"name": "View1",
				"viewName": "View1",
				"targetControl": "app",
				"targetAggregation": "pages"
			},
			{
				"pattern": "welcome",
				"name": "View2",
				"viewName": "MainView",
				"targetControl": "app",
				"targetAggregation": "pages"
			},
			{
				"pattern": "ESS",
				"name": "View3",
				"viewName": "EssView",
				"targetControl": "app",
				"targetAggregation": "pages"
			}]
		}
	}
}